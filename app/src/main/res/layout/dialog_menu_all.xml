<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_centerInParent="true"
        android:gravity="center"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp">
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            app:cardCornerRadius="10dp"
            app:cardElevation="5dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearProfilSekolah"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/profil_sekolah"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Profil\nSekolah"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearAgendaSekolah"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/agenda_sekolah"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Agenda\nSekolah"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearPrestasi"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/prestasi"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Prestasi"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearPPDB"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/ppdb"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="10sp"
                            android:layout_weight="2"
                            android:text="PPDB"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearStrukturOrganisasi"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/struktur_organisasi"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textSize="10sp"
                            android:layout_weight="2"
                            android:text="Struktur\nSekolah"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearJadwalPelajaran"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/jadwal_pelajaran"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Jadwal\nPelajaran"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearEvadir"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/evadir"/>
                        <TextView
                            android:textSize="10sp"
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:text="Evadir"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearMediaPembelajaran"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/media_pelajaran"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Media\nPembelajaran"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearTugas"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/tugas"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Tugas"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearLihatSemua"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/lihat_semua_2"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Lihat\nSemua"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearGuru"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/guru"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Guru"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearBiayaAkademik"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/biaya_akademik"/>
                        <TextView
                            android:textSize="10sp"
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:text="Biaya\nAkademik"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearPembayaran"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/pembayaran"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Pembayaran"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:id="@+id/linearROB"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/rob_dana"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center|top"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="ROB Dana\nBOP"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:visibility="invisible"
                        android:layout_margin="5dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:src="@drawable/lihat_semua_2"/>
                        <TextView
                            android:fontFamily="@font/roboto"
                            android:textStyle="bold"
                            android:textColor="@color/colorBlack"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:textSize="10sp"
                            android:text="Lihat\nSemua"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
</RelativeLayout>